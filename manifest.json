{
	"name": "扇贝+百度查词",
	"version": "0.0.1",
	"manifest_version": 2,
	"description": "使用扇贝网 API 查词，并可添加单词到个人词库",
	"options_page": "options.html",
	"icons": {
		"16": "static/icon16.png",
		"48": "static/icon48.png",
		"128": "static/icon128.png"
	},
	"browser_action": {
		"default_icon": "static/icon48.png",
		"default_popup": "popup.html",
		"default_title": "扇贝查词"
	},
	"permissions": [
		"http://*.shanbay.com/*",
		"http://*.baidu.com/*"
	]
}